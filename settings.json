{
   "code-runner.executorMapByFileExtension": {
        ".sim": "$ECE2612/backdoor/simulate $fileNameWithoutExt",
        ".tcl": "$ECE2612/backdoor/synthesize $fileNameWithoutExt",
        ".m_sim": "$ECE2612/backdoor/ms_simulate $fileNameWithoutExt",
        ".qsf": "$ECE2612/backdoor/q_synthesize $fileNameWithoutExt",        
    },
    "code-runner.fileDirectoryAsCwd": true,
    "code-runner.clearPreviousOutput": true,
    "code-runner.runInTerminal": true,
    "code-runner.terminalRoot": "/"
}
